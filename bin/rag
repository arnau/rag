#!/usr/bin/env ruby
# encoding: UTF-8
#require 'ensure/encoding'
$:.unshift File.expand_path(File.join File.dirname(__FILE__), '..', 'lib')
require 'rag'
require 'rag/cli'

begin
  Rag::Cli.start
# rescue Mam::Error => e
#   exit 0
rescue Interrupt => e
  puts "\nQuitting..."
  puts e.backtrace.join("\n")
  exit 1
end
